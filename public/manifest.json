    },
    "icons": {
    "16": "pwa-64x64.png",
    "32": "pwa-192x192.png",
    "48": "pwa-512x512.png",
    "128": "maskable-icon-512x512.png"
  },
    "oauth2": {
        "client_id": "331595037617-thgohknfla5ehmec4mrdkccm3aq6kvfc.apps.googleusercontent.com",
        "scopes": [
             "https://www.googleapis.com/auth/drive",
             "https://www.googleapis.com/auth/userinfo.email"
        ]
    },
    "permissions": ["activeTab","identity","scripting","identity.email"],
    "host_permissions":[
        "https://*/*",
        "http://*/*"
